<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config/>
    <bean id="credentialsService" class="com.hib.testproject.service.impl.CredentialsServiceImpl"></bean>
    <bean id="passwordStrengthService" class="com.hib.testproject.service.impl.PasswordStrengthServiceImpl"></bean>
    <bean id="credentialsVerifier" class="com.hib.testproject.service.impl.CredentialsVerifierImpl" autowire="byName">
    </bean>
    <bean id="registerService" class="com.hib.testproject.service.impl.RegisterServiceImpl" autowire="byName">
    </bean>


</beans>